
(custom-set-variables
 '(tabbar-background-color "gray20")
 '(tabbar-mode t nil (tabbar))
 '(tabbar-separator (quote (0.8)))
 '(tabbar-use-images t))

(custom-set-faces
 '(tabbar-button ((t (:inherit tabbar-default :foreground "dark red"))))
 '(tabbar-default ((((class color grayscale) (background dark)) (:inherit variable-pitch :height 0.8))))
 '(tabbar-selected ((t (:foreground "white" :height 0.8))))
 '(tabbar-separator ((t (:height 0.1))))
 '(tabbar-unselected ((t (:height 0.8)))))

