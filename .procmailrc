MAILDIR=$HOME/mail/
DEFAULT=$HOME/mail/inbox.spool
VERBOSE=off
LOGFILE=/tmp/procmail.log

:0f
* ^Subject: .*\[Reuse\]
| formail -a "X-RMAIL-KEYWORDS: reuse"

:0f
* ^Subject: .*\[Remind-Fans\]
| formail -a "X-RMAIL-KEYWORDS: remind"

:0f
* ^To: amun-web-list@lists.academymodelun.org
| formail -a "X-RMAIL-KEYWORDS: amun"

:0f
* ^(To|Cc): .*lisp-hug@lispworks.com
| formail -a "X-RMAIL-KEYWORDS: lisp"

:0f
* ^(To|Cc): .*esp.*@mit.edu
| formail -a "X-RMAIL-KEYWORDS: esp"

:0f
* ^(To|Cc): .*@racket-lang.org
| formail -a "X-RMAIL-KEYWORDS: racket"

:0f
* ^(To|Cc): .*hmmt.*@mit.edu
| formail -a "X-RMAIL-KEYWORDS: hmmt"

